{
  "name": "yapu-app-ecommerce",
  "version": "1.0.0",
  "description": "Aplicación móvil de e-commerce desarrollada con React Native y Expo",
  "main": "node_modules/expo/AppEntry.js",
  "keywords": [
    "react-native",
    "expo",
    "e-commerce",
    "mobile-app",
    "redux",
    "navigation"
  ],
  "author": "Yanu",
  "license": "MIT",
  "homepage": "https://yanelyapura.github.io/react-native-expo-features/",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web && node fix-paths.js",
    "deploy:web": "npm run build:web && gh-pages -d dist"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/drawer": "^6.6.7",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/stack": "^6.3.21",
    "@reduxjs/toolkit": "^2.2.1",
    "expo": "~50.0.11",
    "expo-app-loading": "^2.1.1",
    "expo-file-system": "~16.0.8",
    "expo-font": "~11.10.3",
    "expo-image-picker": "~14.7.1",
    "expo-location": "^16.5.5",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "reselect": "^5.1.0",
    "yup": "^1.4.0"
